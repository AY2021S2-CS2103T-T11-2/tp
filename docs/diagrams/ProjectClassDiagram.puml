@startuml
!include style.puml
skinparam arrowThickness 1.1
skinparam arrowColor MODEL_COLOR
skinparam classBackgroundColor MODEL_COLOR

Class ColabFolder
Class ModelManager

Package Project {
Class Project
Class UniqueProjectList
Class EventList
Class DeadlineList
Class TodoList
Class GroupmateList
}

Package Role {
Class Role
}

Package Event {
Class "{abstract}\nRepeatable" as Repeatable
Class Event
}

Package Deadline {
Class "{abstract}\nCompletableDeadline" as CompletableDeadline
Class Deadline
}

Package Todo {
Class "{abstract}\nCompletableTodo" as CompletableTodo
Class Todo
}

Package Groupmate {
Class Groupmate
Class Name
}

ModelManager o--> "1" ColabFolder
ModelManager -->"~* filtered list" Project
ColabFolder *--> "1" UniqueProjectList

UniqueProjectList o-> "*" Project
Project *--> "1" EventList
Project *--> "1" DeadlineList
Project *--> "1" TodoList
Project *--> "1" GroupmateList

EventList o--> "*" Repeatable
Event .up.|> Repeatable

DeadlineList o--> "*" CompletableDeadline
Deadline .up.|> CompletableDeadline

TodoList o--> "*" CompletableTodo
Todo .up.|> CompletableTodo

GroupmateList o--> "*" Groupmate
Groupmate *--> "*" Role
Groupmate *--> Name

@enduml
